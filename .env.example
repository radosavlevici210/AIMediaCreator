# Environment Configuration
NODE_ENV=production
PORT=5000

# Security Configuration
FRONTEND_URL=https://your-domain.replit.app

# Rate Limiting
API_RATE_LIMIT=100
PROJECT_RATE_LIMIT=10

# Monitoring
LOG_LEVEL=info
ENABLE_METRICS=true

# AI Service Keys (Replace with your actual keys)
# OPENAI_API_KEY=your_openai_key_here
# STABILITY_API_KEY=your_stability_key_here
# ELEVEN_LABS_API_KEY=your_eleven_labs_key_here

# Database (if using external database)
# DATABASE_URL=your_database_url_here

# Session Configuration
SESSION_SECRET=your_secure_session_secret_here

# CORS Origins (comma-separated for multiple domains)
ALLOWED_ORIGINS=https://your-domain.replit.app,https://your-custom-domain.com